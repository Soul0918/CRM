<%- model_class = Dazhong -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:chudian) %>:</strong></dt>
  <dd><%= @dazhong.chudian %></dd>
  <dt><strong><%= model_class.human_attribute_name(:openid) %>:</strong></dt>
  <dd><%= @dazhong.openid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:url) %>:</strong></dt>
  <dd><%= @dazhong.url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:time) %>:</strong></dt>
  <dd><%= @dazhong.time %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              dazhongs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_dazhong_path(@dazhong), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              dazhong_path(@dazhong),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
