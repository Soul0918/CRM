<%- model_class = Dazhong -%>
<div class="page-header">
  <h1><%=t 'dazhongs' %></h1>
  <%= search_form_for @search do |f| %>
  <%= f.label :chudian %>
  <%= f.search_field :chudian_cont %>
  <%= f.label :openid %>
  <%= f.search_field :openid_cont %>
  <%= f.submit "search" %>
  <% end %>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:chudian) %></th>
      <th><%= model_class.human_attribute_name(:openid) %></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:time) %></th>
    </tr>
  </thead>
  <tbody>
    <% @dazhongs.each do |dazhong| %>
      <tr>
        <td><%= dazhong.id %></td>
        <td><%= dazhong.chudian %></td>
        <td><%= dazhong.openid %></td>
        <td><%= dazhong.url %></td>
        <td><%= dazhong.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
