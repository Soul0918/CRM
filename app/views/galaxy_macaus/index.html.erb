<%- model_class = GalaxyMacau -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:contact) %></th>
      <th><%= model_class.human_attribute_name(:openid) %></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:time) %></th>
    </tr>
  </thead>
  <tbody>
    <% @galaxy_macaus.each do |galaxy_macau| %>
      <tr>
        <td><%= galaxy_macau.id %></td>
        <td><%= galaxy_macau.contact %></td>
        <td><%= galaxy_macau.openid %></td>
        <td><%= galaxy_macau.url %></td>
        <td><%= galaxy_macau.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
